{
  "name": "dex-trading-system-altseason",
  "version": "2.0.0",
  "description": "ğŸš€ Sistema de Trading DEX Automatizado para Altseason - Bot 24/7 com IA adaptativa, agregaÃ§Ã£o multi-DEX (1inch, Uniswap, Jupiter, PancakeSwap) e dashboard web completo",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" \"npm run dev:trading\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "dev:trading": "cd trading-engine && python src/main.py",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:frontend": "cd frontend && npm start",
    "start:backend": "cd backend && npm start",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install && cd ../trading-engine && pip install -r requirements.txt",
    "migrate": "cd backend && npx prisma migrate dev",
    "migrate:reset": "cd backend && npx prisma migrate reset",
    "seed": "cd backend && npm run seed",
    "test": "npm run test:frontend && npm run test:backend && npm run test:trading",
    "test:frontend": "cd frontend && npm test",
    "test:backend": "cd backend && npm test",
    "test:trading": "cd trading-engine && python -m pytest",
    "lint": "npm run lint:frontend && npm run lint:backend",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:backend": "cd backend && npm run lint",
    "docker:up": "docker-compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker-compose -f docker/docker-compose.yml down",
    "docker:build": "docker-compose -f docker/docker-compose.yml build",
    "backup": "bash scripts/backup.sh",
    "restore": "bash scripts/restore.sh",
    "setup": "bash scripts/setup.sh",
    "deploy": "bash scripts/deploy.sh",
    "create-admin": "cd backend && npm run create-admin",
    "dex-setup": "bash scripts/dex-setup.sh",
    "health-check": "curl http://localhost:3001/api/health && curl http://localhost:3000/api/health"
  },
  "keywords": [
    "dex",
    "trading",
    "bot",
    "cryptocurrency",
    "defi",
    "altseason",
    "1inch",
    "uniswap",
    "jupiter",
    "pancakeswap",
    "automated-trading",
    "machine-learning",
    "blockchain",
    "ethereum",
    "solana",
    "bsc"
  ],
